{
  "name": "@catalystsquad/zest",
  "version": "0.2.0",
  "description": "Library of separated standard web-components and helper libs for everywhere to enjoy, even in frameworks",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run packages",
    "test:watch": "vitest watch packages",
    "build:packages": "npm run build --workspace=packages",
    "test:packages": "vitest run packages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catalystsquad/zest.git"
  },
  "keywords": [
    "webcomponents",
    "vanilla",
    "minimalist"
  ],
  "author": "Catalyst Squad <opensource@catalystsquad.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/catalystsquad/zest/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/catalystsquad/zest#README.md",
  "devDependencies": {
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "happy-dom": "^9.20.3",
    "jsdom": "^22.0.0",
    "vite": "^4.3.2",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@catalystsquad/base-components": ">=0.2.0 <1.0.0",
    "@catalystsquad/basic-data-registry": ">=0.1.1 <1.0.0",
    "@catalystsquad/basic-data-service": ">=0.2.0 <1.0.0",
    "@catalystsquad/date-time-picker": ">=0.2.0 <1.0.0",
    "@catalystsquad/event-broker": ">=0.3.0 <1.0.0",
    "@catalystsquad/menu-gutter": ">=0.2.0 <1.0.0",
    "@catalystsquad/menu-item": ">=0.1.1 <1.0.0",
    "@catalystsquad/menu-logo": ">=0.1.1 <1.0.0",
    "@catalystsquad/simple-router": ">=0.2.2 <1.0.0",
    "@catalystsquad/two-tier-menu": ">=0.2.0 <1.0.0"
  }
